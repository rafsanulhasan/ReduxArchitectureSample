{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=(localdb)\\mssqllocaldb;Initial Catalog=ReduxArchitectureDb;Integrated Security=True;MultipleActiveResultSets=True;"
  },
  "IdentityServer": {
    "Clients": {
      "ReduxArchitecture.AngularApp": {
        "Profile": "IdentityServerSPA"
      },
      "ReduxArchitecture.ReactApp": {
        "Profile": "IdentityServerSPA"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "UseInMemoryDatabase": false
}
